<%= form_with(url: find_car_path(@rental.id)) do |form| %>
  <div>
    <%= form.label :patent, class: "block text-sm font-medium text-gray-700" %>
    <%= form.text_field :patent, 
                placeholder: ('aaa111 o aa111aa'),
                class: "border w-full h-5 px-3 py-5 mt-1 focus:z-10 focus:border-orange-700 focus:outline-none focus:ring-orange-700 rounded-md" %>
  </div>
  <%= button_tag(type: "submit", class: "focus:outline-none text-white bg-blue-400 hover:bg-blue-500 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 dark:focus:ring-blue-900 my-10 py-3") do %>
    <%= content_tag(:strong, t('.desbloquear')) %>
  <% end %>
<% end %>
